@prefix mod: <https://w3id.org/mod#> .
@prefix st: <http://group11.linkeddata.es/ontology/station#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

st: a owl:Ontology ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

#################################
#    Object Properties
#################################

st:inMunicipality a owl:ObjectProperty ;
    rdfs:label "in municipality"@en ;
    rdfs:domain st:Station ;
    rdfs:range st:Province .

st:inProvince a owl:ObjectProperty ;
    rdfs:label "in province"@en ;
    rdfs:domain st:Station ;
    rdfs:range st:Municipality .

#################################
#    Data Properties
#################################

st:hasCode a owl:DatatypeProperty ;
    rdfs:label "has code"@en ;
    rdfs:domain st:Station ;
    rdfs:range xsd:integer .

st:hasDescription a owl:DatatypeProperty ;
    rdfs:label "has description"@en ;
    rdfs:domain st:Station ;
    rdfs:range xsd:string .

st:hasLatitude a owl:DatatypeProperty ;
    rdfs:label "has latitude"@en ;
    rdfs:domain st:Station ;
    rdfs:range xsd:decimal .

st:hasLongitude a owl:DatatypeProperty ;
    rdfs:label "has longitude"@en ;
    rdfs:domain st:Station ;
    rdfs:range xsd:decimal .

st:hasAddress a owl:DatatypeProperty ;
    rdfs:label "has address"@en ;
    rdfs:domain st:Station ;
    rdfs:range xsd:string .

st:hasCP a owl:DatatypeProperty ;
    rdfs:label "has cp"@en ;
    rdfs:domain st:Station ;
    rdfs:range xsd:string .


st:hasPDF a owl:DatatypeProperty ;
    rdfs:label "has pdf"@en ;
    rdfs:domain st:Station ;
    rdfs:range xsd:anyURI .


#################################
#    Classes
#################################

st:Station a owl:Class ;
    rdfs:label "Station"@en .

st:Province a owl:Class ;
    rdfs:label "Province"@en .

st:Municipality a owl:Class ;
    rdfs:label "Municipality"@en .