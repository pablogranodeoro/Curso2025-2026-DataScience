@prefix mod: <https://w3id.org/mod#> .
@prefix st: <http://group11.linkeddata.es/ontology/station#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

st: a owl:Ontology ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

#################################
#    Object Properties
#################################

st:inMunicipality rdf:type owl:ObjectProperty ;
    rdfs:domain st:Station ;
    rdfs:range st:Province ;
    rdfs:label "in municipality" .

st:inProvince rdf:type owl:ObjectProperty ;
    rdfs:domain st:Station ;
    rdfs:range st:Municipality ;
    rdfs:label "in province" .

#################################
#    Data Properties
#################################

st:hasCode rdf:type owl:DatatypeProperty ;
    rdfs:domain st:Station ;
    rdfs:range xsd:integer ;
    rdfs:label "has code" .

st:hasDescription rdf:type owl:DatatypeProperty ;
    rdfs:domain st:Station ;
    rdfs:range xsd:string ;
    rdfs:label "has description" .

st:hasLatitude rdf:type owl:DatatypeProperty ;
    rdfs:domain st:Station ;
    rdfs:range xsd:decimal ;
    rdfs:label "has latitude" .

st:hasLongitude rdf:type owl:DatatypeProperty ;
    rdfs:domain st:Station ;
    rdfs:range xsd:decimal ;
    rdfs:label "has longitude" .

st:hasAddress rdf:type owl:DatatypeProperty ;
    rdfs:domain st:Station ;
    rdfs:range xsd:string ;
    rdfs:label "has address" .

st:hasCP rdf:type owl:DatatypeProperty ;
    rdfs:domain st:Station ;
    rdfs:range xsd:string ;
    rdfs:label "has cp" .

st:hasPDF rdf:type owl:DatatypeProperty ;
    rdfs:domain st:Station ;
    rdfs:range xsd:anyURI ;
    rdfs:label "has pdf" .

#################################
#    Classes
#################################

st:Station rdf:type owl:Class ;
    rdfs:label "Station" .

st:Province rdf:type owl:Class ;
    rdfs:label "Province" .

st:Municipality rdf:type owl:Class ;
    rdfs:label "Municipality" .