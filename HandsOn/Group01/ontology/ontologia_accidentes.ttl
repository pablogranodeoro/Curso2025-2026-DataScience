@prefix : <http://groupXX.linkeddata.es/accidentes/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://groupXX.linkeddata.es/accidentes/ontology/> .

<http://groupXX.linkeddata.es/accidentes/ontology> rdf:type owl:Ontology ;
    rdfs:label "Ontología de Accidentes de Tráfico - Madrid 2025"@es .

#Clases
:Accidente rdf:type owl:Class ; rdfs:label "Accidente de tráfico"@es .
:Clima rdf:type owl:Class ; rdfs:label "Condición atmosférica"@es .
:Distrito rdf:type owl:Class ; rdfs:label "Distrito municipal"@es .
:Lesion rdf:type owl:Class ; rdfs:label "Tipo de lesión"@es .
:Persona rdf:type owl:Class ; rdfs:label "Persona implicada"@es .
:RangoEdad rdf:type owl:Class ; rdfs:label "Franja de edad"@es .
:Vehiculo rdf:type owl:Class ; rdfs:label "Tipo de vehículo"@es .

#Propiedades
:involucraPersona rdf:type owl:ObjectProperty ;
    rdfs:domain :Accidente ; rdfs:range :Persona ;
    rdfs:label "involucra a persona"@es .

:involucraVehiculo rdf:type owl:ObjectProperty ;
    rdfs:domain :Accidente ; rdfs:range :Vehiculo ;
    rdfs:label "involucra a vehículo"@es .

:ocurreEn rdf:type owl:ObjectProperty ;
    rdfs:domain :Accidente ; rdfs:range :Distrito ;
    rdfs:label "ocurre en distrito"@es .

:tieneCondicionMeteorologica rdf:type owl:ObjectProperty ;
    rdfs:domain :Accidente ; rdfs:range :Clima ;
    rdfs:label "condición meteorológica"@es .

:tieneLesion rdf:type owl:ObjectProperty ;
    rdfs:domain :Persona ; rdfs:range :Lesion ;
    rdfs:label "presenta lesión"@es .

:tieneRangoEdad rdf:type owl:ObjectProperty ;
    rdfs:domain :Persona ; rdfs:range :RangoEdad ;
    rdfs:label "rango de edad"@es .

:tieneCoordenadaX rdf:type owl:DatatypeProperty ;
    rdfs:domain :Accidente ; rdfs:range xsd:float ;
    rdfs:label "coordenada X"@es .

:tieneCoordenadaY rdf:type owl:DatatypeProperty ;
    rdfs:domain :Accidente ; rdfs:range xsd:float ;
    rdfs:label "coordenada Y"@es .

:tieneFecha rdf:type owl:DatatypeProperty ;
    rdfs:domain :Accidente ; rdfs:range xsd:date ;
    rdfs:label "fecha del accidente"@es .

:tieneHora rdf:type owl:DatatypeProperty ;
    rdfs:domain :Accidente ; rdfs:range xsd:string ;
    rdfs:label "hora del accidente"@es .

:tienePositivoAlcohol rdf:type owl:DatatypeProperty ;
    rdfs:domain :Persona ; rdfs:range xsd:boolean ;
    rdfs:label "positivo en alcohol"@es .

:tienePositivoDroga rdf:type owl:DatatypeProperty ;
    rdfs:domain :Persona ; rdfs:range xsd:boolean ;
    rdfs:label "positivo en drogas"@es .

:tieneSexo rdf:type owl:DatatypeProperty ;
    rdfs:domain :Persona ; rdfs:range xsd:string ;
    rdfs:label "sexo"@es .

#Ejemplo

:Accidente_2025S000001 rdf:type owl:NamedIndividual , :Accidente ;
    :involucraPersona :Persona_1 ;
    :involucraVehiculo :Vehiculo_Turismo ;
    :ocurreEn :Distrito_CiudadLineal ;
    :tieneCondicionMeteorologica :Clima_Lluvioso ;
    :tieneFecha "2025-01-27"^^xsd:date ;
    :tieneHora "20:50:00" ;
    :tieneCoordenadaX 445270 ;
    :tieneCoordenadaY 4475979 ;
    rdfs:label "Atropello a persona en Ciudad Lineal"@es .

:Persona_1 rdf:type owl:NamedIndividual , :Persona ;
    :tieneSexo "Hombre" ;
    :tieneRangoEdad :RangoEdad_30_34 ;
    :tieneLesion :Lesion_SinAsistencia ;
    :tienePositivoAlcohol false ;
    :tienePositivoDroga true ;
    rdfs:label "Conductor involucrado"@es .

:Vehiculo_Turismo rdf:type owl:NamedIndividual , :Vehiculo ;
    rdfs:label "Turismo"@es .

:Distrito_CiudadLineal rdf:type owl:NamedIndividual , :Distrito ;
    rdfs:label "Ciudad Lineal"@es .

:Clima_Lluvioso rdf:type owl:NamedIndividual , :Clima ;
    rdfs:label "Lluvioso"@es .

:Lesion_SinAsistencia rdf:type owl:NamedIndividual , :Lesion ;
    rdfs:label "Sin asistencia sanitaria"@es .

:RangoEdad_30_34 rdf:type owl:NamedIndividual , :RangoEdad ;
    rdfs:label "De 30 a 34 años"@es .
