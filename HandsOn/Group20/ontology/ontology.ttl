@prefix ns:   <https://datos.grupo20-2025-cdia.org/ontology/bicimad#> .
@prefix ex:   <https://datos.grupo20-2025-cdia.org/resource/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

<https://datos.grupo20-2025-cdia.org/ontology/bicimad> a owl:Ontology ;
    rdfs:label "Ontología ligera de BiciMAD"@es ;
    rdfs:comment "Ontología mínima para estaciones de BiciMAD y su localización administrativa. Cumple las normas de nomenclatura, dominios y rangos de la asignatura."@es .

############################
# Clases
############################

ns:Station  a owl:Class ;
    rdfs:label "Estación"@es ;
    rdfs:comment "Estación de BiciMAD."@es .

ns:Location a owl:Class ;
    rdfs:label "Localización"@es ;
    rdfs:comment "Coordenadas geográficas de una estación."@es .

ns:District a owl:Class ;
    rdfs:label "Distrito"@es ;
    rdfs:comment "Unidad administrativa del municipio de Madrid."@es .

############################
# Propiedades de objeto
############################

ns:hasLocation a owl:ObjectProperty ;
    rdfs:label "ubicación"@es ;
    rdfs:domain ns:Station ;
    rdfs:range  ns:Location .

ns:hasDistrict a owl:ObjectProperty ;
    rdfs:label "distrito"@es ;
    rdfs:domain ns:Station ;
    rdfs:range  ns:District .

############################
# Propiedades de datos (Station)
############################

ns:hasStationId a owl:DatatypeProperty ;
    rdfs:label "id de la estación"@es ;
    rdfs:domain ns:Station ;
    rdfs:range  xsd:string .

ns:hasName a owl:DatatypeProperty ;
    rdfs:label "nombre"@es ;
    rdfs:domain ns:Station ;
    rdfs:range  xsd:string .

ns:hasAddress a owl:DatatypeProperty ;
    rdfs:label "dirección"@es ;
    rdfs:domain ns:Station ;
    rdfs:range  xsd:string .

ns:hasDockBikes a owl:DatatypeProperty ;
    rdfs:label "bicicletas enganchadas"@es ;
    rdfs:domain ns:Station ;
    rdfs:range  xsd:integer .

ns:hasFreeBases a owl:DatatypeProperty ;
    rdfs:label "bases libres"@es ;
    rdfs:domain ns:Station ;
    rdfs:range  xsd:integer .

ns:hasTotalBases a owl:DatatypeProperty ;
    rdfs:label "bases totales"@es ;
    rdfs:domain ns:Station ;
    rdfs:range  xsd:integer .

ns:hasStatus a owl:DatatypeProperty ;
    rdfs:label "estado"@es ;
    rdfs:domain ns:Station ;
    rdfs:range  xsd:string .

############################
# Propiedades de datos (Location)
############################

ns:hasLatitude a owl:DatatypeProperty ;
    rdfs:label "latitud"@es ;
    rdfs:domain ns:Location ;
    rdfs:range  xsd:float .

ns:hasLongitude a owl:DatatypeProperty ;
    rdfs:label "longitud"@es ;
    rdfs:domain ns:Location ;
    rdfs:range  xsd:float .
